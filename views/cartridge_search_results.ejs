<% include partials/header %>
<section>
    <div>
        <% if(res != null) { %>
        <table>
            <tr>
                <td>Case</td>
                <td>Projectile</td>
                <td>Powder</td>
                <td>Grains</td>
                <td>Velocity</td>
            </tr>
            <% for (var i=0; res.length > i; i++) { %>
            <tr>
                <td><%= res[i].name %></td>
                <td><%= res[i].proj_weight %> grns <%= res[i].proj_type %></td>
                <td><%= res[i].manuf %> <%= res[i].powder %></td>
                <td><%= res[i].powder_weight %></td>
                <td><%= res[i].velocity %></td>
            </tr>
            <% } %>
        </table>
        <% } else { %>
        No loads exist.<br />
        <% } %>
        <br />
        <a href="/cdb/cartridgesAll">Go Back</a>
    </div>
</section>
<% include partials/footer %>